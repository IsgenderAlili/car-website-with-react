{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "/.", "/node_modules/*"],
    "cleanUrls": true,
    "trailingSlash": false,
    "appAssociation": "AUTO"
  },
  "rewrites": [
    {
      "source": "**",
      "destination": "./public/index.html",
      "dynamicLinks": true
    }
  ]
}
